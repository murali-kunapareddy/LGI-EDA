<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>LGI > EDA > @ViewData["Title"]</title>
    <link href="https://cdn.lineicons.com/4.0/lineicons.css" rel="stylesheet" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/EDA.styles.css" asp-append-version="true" />
</head>
<body>
    <div id="wrapper" class="active">
        <!-- Sidebar -->
        <div id="sidebar-wrapper">
            <ul id="sidebar_menu" class="sidebar-nav">
                <li class="sidebar-brand color-blue"><a id="menu-toggle" href="#">LGI <i class="lni lni-menu float-right"></i></a></li>
            </ul>
            <ul class="sidebar-nav" id="sidebar">
                <li class="color-white"><a>Home <i class="lni lni-home float-right"></i></a></li>
                <li class="color-white"><a>Orders <i class="lni lni-ticket-alt float-right"></i></a></li>
                <li class="color-white"><a>Customers <i class="lni lni-users float-right"></i></a></li>
                <li class="color-white">
                    <a>Masters <i class="lni lni-list float-right"></i></a>
                    <ul>
                        <li><a style="width:148px; color:antiquewhite; text-indent: 10px;" href="#">Companies</a></li>
                        <li><a style="width:148px; color:antiquewhite; text-indent: 10px;" href="#">Consignee Types</a></li>
                        <li><a style="width:148px; color:antiquewhite; text-indent: 10px;" href="#">Container Sizes</a></li>
                        <li><a style="width:148px; color:antiquewhite; text-indent: 10px;" href="#">Countries</a></li>
                        <li><a style="width:148px; color:antiquewhite; text-indent: 10px;" href="#">Incoterms</a></li>
                        <li><a style="width:148px; color:antiquewhite; text-indent: 10px;" href="#">Paperworks List</a></li>
                        <li><a style="width:148px; color:antiquewhite; text-indent: 10px;" href="#">Payment Terms</a></li>
                        <li><a style="width:148px; color:antiquewhite; text-indent: 10px;" href="#">Plant Locations</a></li>
                        <li><a style="width:148px; color:antiquewhite; text-indent: 10px;" href="#">Ports</a></li>
                        <li><a style="width:148px; color:antiquewhite; text-indent: 10px;" href="#">Products</a></li>
                        <li><a style="width:148px; color:antiquewhite; text-indent: 10px;" href="#">Ship Via</a></li>

                    </ul>
                </li>
                <li class="color-white"><a>Comm Center <i class="lni lni-popup float-right"></i></a></li>
                <li class="color-white"><a>Users <i class="lni lni-user float-right"></i></a></li>
            </ul>
        </div>

        <!-- Page content -->
        <div class="container">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>
        <footer class="border-top footer text-muted">
            <div class="container">
                &copy; 2024 - EDA - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
            </div>
        </footer>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
