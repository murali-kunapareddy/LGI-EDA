<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>LGI > EDA > @ViewData["Title"]</title>
    <link href="https://cdn.jsdelivr.net/npm/boxicons@latest/css/boxicons.min.css" rel="stylesheet" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/EDA.styles.css" asp-append-version="true" />

</head>
<body id="body-pd">
    <div class="flex-wrapper">
        <header class="header" id="header">
            <div class="header_toggle"> <i class='bx bx-menu' id="header-toggle"></i> </div>
            <div class="header_title">Export & Domestic Application</div>
            <div class="header_img"> <img src="~/images/KMK.jpg" alt=""> </div>
        </header>
        <div class="l-navbar" id="nav-bar">
            <nav class="nav">
                <div>
                    <a href="#" class="nav_logo"> <i class='bx bx-layer nav_logo-icon'></i> <span class="nav_logo-name">The Lauridsen Group</span> </a>
                    <div class="nav_list">
                        <a href="#" class="nav_link active text-white"> <i class='bx bx-grid-alt nav_icon'></i> <span class="nav_name">Dashboard</span> </a>
                        <a href="#" class="nav_link text-white">  <i class='bx bx-bar-chart-alt-2 nav_icon'></i> <span class="nav_name">Orders</span> </a>
                        <a href="#" class="nav_link text-white"> <i class='bx bx-group nav_icon'></i> <span class="nav_name">Customers</span> </a>
                        <a href="#" class="nav_link text-white"> <i class='bx bx-bookmark nav_icon'></i> <span class="nav_name">Masters</span> </a>
                        <a href="#" class="nav_link text-white"> <i class='bx bx-folder nav_icon'></i> <span class="nav_name">Comm Center</span> </a>
                        <a href="#" class="nav_link text-white"> <i class='bx bx-user nav_icon'></i> <span class="nav_name">Users</span> </a>
                    </div>
                </div>
                <a href="#" class="nav_link text-white"> <i class='bx bx-log-out nav_icon'></i> <span class="nav_name">SignOut</span> </a>
            </nav>
        </div>
        <div class="content">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>
        <div class="footer">
            <div class="container">
                &copy; 2024 - EDA - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
            </div>
        </div>
    </div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>



    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
